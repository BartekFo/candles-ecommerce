    "@next/bundle-analyzer": "^12.1.6",
    "@svgr/webpack": "^6.2.1",
    "@typescript-eslint/parser": "^5.26.0",
    "daisyui": "^2.15.2",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase": "^9.8.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "next": "12.1.6",
    "next-compose-plugins": "^2.2.1",
